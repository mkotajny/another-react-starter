		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^19.0.1",
		"eslint-config-prettier": "^8.3.0",
