		"@testing-library/user-event": "^12.8.3",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
